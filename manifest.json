{
  "name": "Explode",
  "version": "0.5",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Expand shortened URLs (e.g. TinyURL) with LongURL",
  "background_page": "background.html",
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [ "explode.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
    }
  ],
  "permissions": [
    "tabs",
    "http://api.longurl.org/*"
  ]
}
